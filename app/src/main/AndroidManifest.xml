                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.evgenii.jsevaluatortests.MainActivity" />
        </activity>
        <activity
            android:name=".StressTestActivity"
            android:label="@string/title_activity_stress_test"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.evgenii.jsevaluatortests.MainActivity" />
        </activity>
        <activity
            android:name=".RealLibrary"
            android:label="@string/title_activity_real_library"
